# Alpha Vantage API Configuration
# Get your free API key from: https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# AWS Configuration (for Lambda/S3 backtesting)
# These are optional - only needed if using AWS Lambda for backtesting
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_REGION=us-east-1
AWS_S3_BUCKET=initial-data-01

# AWS Lambda Configuration (optional)
# This is automatically set when running in AWS Lambda
# AWS_LAMBDA_FUNCTION_NAME=your_lambda_function_name

# Application Configuration
# API call delay in seconds (to respect rate limits)
CALL_DELAY_SECONDS=15
